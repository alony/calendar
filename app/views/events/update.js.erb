$('.event_dialog').dialog('close');
$('td#<%= @prev_date ? @prev_date.strftime('%m_%d_%Y') : "no_id"-%>').removeClass("user event");
$('td#<%= @event.event_dt.strftime('%m_%d_%Y')-%>').addClass("<%= @event.users.include?(current_user) ? "user" : ""%> event").find("a").attr("href", "<%=edit_event_path(@event)-%>");
